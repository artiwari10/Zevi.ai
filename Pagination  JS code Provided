const products = ["p1", "p2", "p3", "p4", "p5"];
const blogs = ["b1", "b2", "b3", "b4", "b5", "b6", "b7", "b7", "b8"];

const getProducts = (from, limit) => {
  return {
    count: products.length,
    result: products.slice(from - 1, from - 1 + limit)
  }
};

const getBlogs = (from, limit) => {
  return {
    count: blogs.length,
    result: blogs.slice(from - 1, from - 1 + limit)
  };
};
//////////////////////////////////////////////////////////////
////////////////// NO CHANGES REQUIRED HERE //////////////////

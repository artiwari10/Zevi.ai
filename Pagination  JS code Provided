  console.log(getProducts(1,2))
  console.log(getBlogs(4,3))
}



//////////////////// WHITE YOUR CODE HERE ////////////////////


paginate(1,3) // [p1, p2, p3]
paginate(2,3) // [p4, p5, b1]
paginate(3,3) // [b2, b3, b4]





